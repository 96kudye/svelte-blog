        "typescript": "^5.0.0",
        "vite": "^4.3.9"
    },
    "type": "module",
    "dependencies": {
        "@vercel/analytics": "^1.0.1",
        "@vercel/speed-insights": "^1.0.1",
        "cheerio": "^1.0.0-rc.12",
        "dayjs": "^1.11.7",
        "highlight.js": "^11.8.0",
        "microcms-js-sdk": "^2.4.0"
    }
